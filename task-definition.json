  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::741783034843:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "baby-raffle-api",
      "image": "741783034843.dkr.ecr.us-east-1.amazonaws.com/baby-raffle-api:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
